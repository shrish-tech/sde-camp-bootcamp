// Check age is greater than 18(condition statement)

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner scn = new Scanner(System.in);
		int age = scn.nextInt();
		
		if(age >= 18){
		    System.out.println("Age greater than 18, can vote.");
		}else{
		    System.out.println("Can't vote, Wait till you become 18 :)");
		}
	}
}

//Input:
19

//Output:
Age greater than 18, can vote.
